    @setvar! waitingForCursor NOT_WAITING
endif
if not varexist poisonstrike
    @setvar! poisonstrike 2
endif
if not cooldown "Poison strike"
    @setvar! waitingForCursor poisonstrike
    say '[poisonstrike'
    while not targetexists 
    endwhile
    while targetexists 
    endwhile
    pause 100
