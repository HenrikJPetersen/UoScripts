clearsysmsg 
if not varexist cursorWait
    @setvar! cursorWait NOT_WAITING
endif
if not varexist teleport
    @setvar! teleport 1
endif
cast 'Teleport'
@setvar! cursorWait teleport
while not targetexists and not insysmsg 'Your concentration is disturbed, thus ruining thy spell'
    pause 25
    if insysmsg 'Your concentration is disturbed, thus ruining thy spell'
        overhead 'Disturbed'
        @setvar! cursorWait NOT_WAITING
        break
    endif
endwhile
while targetexists   
endwhile
if cursorWait == teleport and not insysmsg 'Your concentration is disturbed, thus ruining thy spell'
    cooldown "Teleport" 15
    @setvar! cursorWait NOT_WAITING
endif
